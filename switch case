#include <stdio.h>
int main (){
    int a,b;
    char op;
    printf("Enter the value of a\n");
    scanf("%d",&a);
    printf("Enter the value of b\n");
    scanf("%d",&b);
    printf("Enter the operator value of\n");
    scanf("\n%c",&op);
    switch(op)
    {
    
     case '+':
    printf("%d addition is", a+b);
    break;
    case '-':
    printf("%d subraction is", a-b);
    break;
    case '*':
    printf("%d multiplication is", a*b);
    break;
    case '/':
    printf("%d division is ",a/b);
    break;
        
    }
    return 0;
}
